{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/windows/wsl/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "ms.prod" : "windows-subsystem-for-linux"
    },
    "fileMetadata": {
        "open_to_public_contributors": {
		        "./*.md":  true,
        }
    },
    "template": [],
    "dest": "wsl",
    "markdownEngineName": "markdig"
  }
}
